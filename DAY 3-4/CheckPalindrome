class PalindromeNo {
    public static void main(String[] args) {
        int n = 45654;
        int temp = n;
        int rev = 0;
      while (n != 0) {
            int digit = n % 10;
            rev = rev * 10 + digit;
            n = n / 10;
        }
 if (temp == rev) {
     System.out.println("Palindrome Number");
       } else {
    System.out.println("Not a Palindrome Number");
        }
    }
}
